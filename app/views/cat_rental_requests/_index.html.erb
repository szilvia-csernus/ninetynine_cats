<h2><%= @cat.name %>'s rental requests</h2>

<table>
    <thead>
        <tr>
            <th>Start date</th>
            <th>End date</th>
            <th>Status</th>
            <th>Requester</th>
        </tr>
    </thead>
    <% @cat_rental_requests.each do |cat_r_r| %>
    <tbody>
        <tr>
            <td><%= cat_r_r.start_date %></td>
            <td><%= cat_r_r.end_date %></td>
            <td><%= cat_r_r.status %></td>
            <td><%= cat_r_r.requester.username %>
            <td><% if (@owner_call && cat_r_r.pending?) %>
                <a href="<%= approve_cat_rental_request_url(cat_r_r) %>">Approve Request </a>
                <a href="<%= deny_cat_rental_request_url(cat_r_r) %>">Deny Request</a>
                <% end %></a>
            </td>
        </tr>
    </tbody>
    <% end %>
</table>



